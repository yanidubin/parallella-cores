SAPI=1
[main]
name = parallella
description = "Parallalla 7020 based backer board"

backend = ise

[ise]
ucf_files = 
 boards/parallella-I/constraints/parallella_timing.ucf
 boards/parallella-I/constraints/parallella_z7020_loc.ucf
 boards/parallella-I/constraints/parallella_z70x0_loc.ucf

family = zynq
device = xc7z020
package = clg400
speed = -1
top_module = parallella_z7_top

global_includes =
 fpga/projects/parallella_7020_headless/version.v
 fpga/hdl/parallella-I/fpga_constants.v

tcl_files =
 tcl/synth.tcl

system_files =
 fpga/edk/parallella_7020_headless/system.mhs
 fpga/edk/parallella_7020_headless/data/ps7_system_prj.xml

custom_scripts =
 scripts/run_synthesis.xst

# Not present in sources - generated by one of the steps
generated_srcs =
 hdl/system.v
